{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "A simple yet complete webpack boilerplate.",
  "main": "build/main",
  "scripts": {
    "build": "WEBPACK_ENV=production webpack --progress --hide-modules --config config/webpack.config.js",
    "dev": "NODE_ENV=dev webpack-dev-server --progress --content-base ./src --config config/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
