{
  "name": "gulp-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A simple yet complete webpack boilerplate.",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "dev": "NODE_ENV=development concurrently \"npm start\" \"gulp\"",
    "webpack": "WEBPACK_ENV=production webpack --progress --hide-modules --config config/webpack.config.js",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --progress --content-base ./src --config config/webpack.config.js",
    "clean": "rm -r dist && rm -r build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "Alex Grozav (Pixevil)",
