{
  "name": "gulp-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A simple yet complete webpack boilerplate.",
  "main": "server.js",
  "scripts": {
    "build": "WEBPACK_ENV=production webpack --progress --hide-modules --config config/webpack.config.js",
    "dev": "NODE_ENV=development webpack-dev-server --progress --content-base ./src --config config/webpack.config.js",
    "clean": "rm -r dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "Alex Grozav (Pixevil)",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.52",
    "@types/node": "^7.0.5",
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "csslint": "^0.10.0",
    "csslint-loader": "^1.0.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "ie-shim": "^0.1.0",
    "image-webpack-loader": "^3.2.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "poststylus": "^0.2.3",
    "pug": "^2.0.0-beta3",
    "pug-loader": "^2.3.0",
    "rollup": "^0.41.4",
    "rollup-loader": "^0.1.2",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-webpack-loader": "^1.0.0",
    "rucksack-css": "^0.9.1",
    "rxjs": "^5.1.1",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.1.6",
