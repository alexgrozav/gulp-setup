    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.28.0",
    "ie-shim": "^0.1.0",
    "lodash": "^4.17.4",
    "poststylus": "^0.2.3",
    "pug": "^2.0.0-beta3",
    "pug-loader": "^2.3.0",
    "rucksack-css": "^0.9.1",
